{
  "expo": {
    "name": "Brewed Confessions",
    "slug": "my-goals",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/teabook.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/readingbook.png",
        "backgroundColor": "#FFE6E6"
      },
      "package": "com.caramelanneee.mygoals"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/teabook.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/readingbook.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#FFE6E6"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "ce1e1b2c-e512-43cb-a904-fb8e2a673b51"
      }
    }
  }
}
